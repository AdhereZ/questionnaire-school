.Frame {
  position: relative;
  background-color: white;
  left: 0rpx;
  line-height: 40rpx;
  box-shadow: 0rpx 12rpx 28rpx -20rpx rgba(0, 0, 0, 0.4);
  margin-bottom: 20rpx;
  padding: 24rpx 52rpx 30rpx 26rpx;
}

.Frame .Title {
  display: block;
  text-overflow: ellipsis;
  word-wrap: break-word;
  width: 364rpx;
  line-height: 45rpx;
  color: #101010;
  font-size: 28rpx;
  font-weight: 700;
  text-align: left;
  font-family: PingFangSC-bold;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box !important;
  -webkit-line-clamp: 2 !important;
  -webkit-box-orient: vertical;
}

.Frame .Gain {
  vertical-align: top;
  width: 98rpx;
  height: 40rpx;
  color: #ed4f4f;
  font-size: 28rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}

.Frame .Text {
  display: block;
  text-overflow: ellipsis;
  word-wrap: break-word;
  width: 690rpx;
  line-height: 37rpx;
  color: #101010;
  font-size: 24rpx;
  text-align: left;
  display: -webkit-box;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-family: PingFangSC-regular;
  margin-bottom: 28rpx;
}

.Frame .Ques {
  /* top: 10rpx;
  position: relative;
  left: 30rpx; */
  width: 98rpx;
  height: 34rpx;
  margin-right: 32rpx;
  line-height: 40rpx;
  text-align: center;
  box-shadow: 0rpx 0rpx 12rpx -6rpx rgba(0, 0, 0, 0.4);
  color: #A59F9F;
  font-size: 24rpx;
}

.Frame .Remain {
  /* position: relative;
  left: 160rpx;
  top: -22rpx; */
  width: 87rpx;
  height: 34rpx;
  line-height: 40rpx;
  text-align: center;
  box-shadow: 0rpx 0rpx 12rpx -6rpx rgba(0, 0, 0, 0.4);
  color: #2D77FB;
  font-size: 24rpx;
}

.Frame .Type {
  top: -60rpx;
  left: 610rpx;
  position: relative;
  width: 104rpx;
  height: 38rpx;
  color: #696565;
  font-size: 26rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}
.dark {
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: #fff;
  opacity: 0.5;
  top: 0;
  left: 0;
  z-index: 1;
}