page {
    background-color: #FCFCFC;
    padding-bottom: 100rpx;
}

/* 我发布的 */
.finished {
   margin-top: 44rpx;
   margin-bottom: 100rpx;
}

.createdBar {
    padding: 18rpx 40rpx 0 30rpx;
    height: 76rpx;
    font-size: 32rpx;
    font-weight: 700;
    font-family: PingFangSC-bold;
    background-color: rgba(248, 255, 255, 100);
}

.express {
   float: right;
   width: 40rpx;
   height: 40rpx;
   border: 2rpx solid #000;
   border-radius: 20rpx;
   display: flex;
  justify-content: center;
  align-items: center;
   background-color: rgba(248, 255, 255, 100);
}
.cf {
    width: 18rpx;
    height: 18rpx;
}

.createdQA {  
    width: 720rpx;
    margin: 20rpx 16rpx 0 16rpx;
    background-color: #fff;
    box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2)
}
.allCondition {
    position: relative;
    padding: 20rpx 0;
    width: 100%;
    border-radius: 20rpx;
}
.lastOperationtime {
    position: absolute;
    margin-left: 32rpx;
    font-size: 24rpx;
}
.condition {
    position: absolute;
    right: 72rpx;
    margin-right: 30rpx;
    font-size: 24rpx;
    font-weight: 700;
    color :#FF9704;
}

.qTitle {
    margin: 40rpx 0 0 32rpx;
    height: 66rpx;
    font-size: 40rpx;
    text-align: left;
    font-weight: 700;
}
.finishNum {
    margin: 8rpx 0 0 32rpx;
    font-size: 24rpx;
}
.btns {
    margin-top: 25rpx;
}
.btn {
    display: inline;
    border: 1px solid #2D77FB;
    color: #2D77FB;
    background-color: #fff;
    line-height: 35rpx;
    border-radius: 8rpx;
    font-size: 24rpx;
    font-weight: normal;
    text-align: center;
    font-family: Arial;
}
.btn:nth-child(1) {
    /* width: 140rpx; */
    margin-left: 30rpx;
    padding: 6rpx 46rpx;
}
.btn:nth-child(2) {
    /* width: 116rpx; */
    margin-left: 212rpx;
    padding: 6rpx 32rpx;
}
.btn:nth-child(3) {
    margin-left: 32rpx;
    /* width: 146rpx; */
    margin-right: 10rpx;
    padding: 6rpx 24rpx;
}



