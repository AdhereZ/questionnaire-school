.com-selectBox{
    width: 140rpx;
    height: 54rpx;

}
.com-sContent{
    border-radius: 10rpx;
    border: 2rpx solid #BBBBBB;
    background: white;
    font-size: 24rpx;
    position: relative;
    height: 60rpx;
    line-height: 60rpx;
}
.com-sImg{
    position: absolute;
    right: 10rpx;
    top: 22rpx;
    width: 18rpx;
    height: 18rpx;
    transition: all .3s ease;
}
.com-sTxt{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding:0 40rpx 0 12px;
    font-size: 24rpx;
}
.com-sList{
    background: white;
    width: inherit;
    position: absolute;
    border: 2rpx solid #e2e2e2;
    border-top: none;
    box-sizing: border-box;
    z-index: 3;
    max-height: 240rpx;
    overflow: auto;
}
.com-sItem{
    height: 60rpx;
    line-height: 60rpx;
    border-top: 2rpx solid #e2e2e2;
    padding: 0 26rpx;
    text-align: left;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 24rpx;
}
.com-sItem:first-child{
    border-top: none;
}

.bgColor {
    background-color: #5677fc;
    color: #fff;
}