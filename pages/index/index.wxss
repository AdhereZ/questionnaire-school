.cu-bar {
    position: fixed;
    display: flex;
    bottom: 0;
    left: 0;
    width: 100%;
    margin-bottom: 0;
    box-shadow: 0px -2px 5px -4px rgba(0, 0, 0, 0.4);
    z-index: 2;
}

.box {
    flex: 1;
    justify-content: center;
    align-items: center;
}

.tabbar-img {
    width: 46rpx;
    height: 46rpx;
}

.tabbar-text {
    margin-top: 10rpx;
    font-size: 24rpx;

}

.add-action {
    position: relative;
}
.tabbar-center-img {
    position: absolute;
    bottom: 48rpx;   
    left: 48rpx;
    z-index: 1;
    width: 52rpx;
    height: 52rpx;
}

.cu-dialog {
    background-color: #fff;
    height: 356rpx;
}

.cu-dialog .createb {
    padding: 25rpx 0 0 40rpx;
    display: flex;
    align-items: center;
    float: left;
    height: 178rpx;
    width: 340rpx;
    color: rgba(159, 159, 165, 100);
}


.cu-dialog .createb text {
    display: block;
    margin-left: 30rpx;
}

.createb-img {
    width: 136rpx;
}
