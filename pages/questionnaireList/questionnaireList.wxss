page {
    background-color: #FCFCFC;
    padding-bottom: 100rpx;
}

/* 我的问卷 */
.finished {
    margin-top: 44rpx;
}

.createdBar {
    padding: 18rpx 40rpx 0 30rpx;
    height: 76rpx;
    font-size: 32rpx;
    font-weight: 700;
    font-family: PingFangSC-bold;
    background-color: rgba(248, 255, 255, 100);
}

.express {
    float: right;
    width: 40rpx;
    height: 40rpx;
    border: 2rpx solid #000;
    border-radius: 20rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: rgba(248, 255, 255, 100);
}

.cf {
    width: 18rpx;
    height: 18rpx;
}


.createdQA {
    display: inline-block;
    width: 346rpx;
    margin: 20rpx 0rpx 20rpx 16rpx;
    padding-top: 20rpx;
    border-radius: 10rpx;
    background-color: #fff;
    box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2)
}

.box1:nth-child(2n-1) {
    margin-left: 18rpx;
}

.box1:nth-child(2n) {
    margin-right: 24rpx;
}

.lastOperationtime {
    margin-left: 32rpx;
    font-size: 24rpx;
    margin-right: 60rpx;
}

.qTitle {
    margin: 20rpx 0 0 32rpx;
    height: 66rpx;
    line-height: 66rpx;
    font-size: 40rpx;
    text-align: left;
    font-weight: 700;
    display: -webkit-box;
    text-overflow: ellipsis;
    overflow: hidden;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
}

.finishPercent {
    margin-right: 70rpx;
    color: #626262;
    font-size: 24rpx;
}

.queestionNum {
    color: #626262;
    font-size: 24rpx;
    width: 150rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: right;
    color: #2d79fb;
}



